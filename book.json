{
    "title": "学习笔记",
    "author": "援桌骑士宁宁",
    "outputfile": "./SUMMARY.md",
    "catalog": "all",
    "ignores": ["_book", "node_modules","docs"],
    "unchanged": [],
    "sortedBy": [],
    "disableTitleFormatting": true,
    "plugins": ["mermaid-gb3","chapter-fold","back-to-top-button","code","splitter","-lunr","yahei","theme-comscore","autotheme",
      "-search","search-pro","pageview-count","tbfed-pagefooter","popup","-sharing","advanced-emoji","klipse","alerts","hide-element",
    "page-toc-button"],
    "pluginsConfig": {
      "theme-default": {
           "showLevel": true
      },
      "tbfed-pagefooter": {
        "copyright":"Ciallo",
        "modify_label": "该文章修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element":{
      "elements": [".gitbook-link",".navigation-prev",".navigation-next",".copyright"]
    },
    "fontSettings": {
      "family": "msyh",
      "size": 2
    },
    "autotheme": {
      "white": [9, 10, 11, 12, 13, 14, 15, 16],
      "sepia": [6, 7, 8, 17, 18, 19],
      "night": [20, 21, 22, 23, 0, 1, 2, 3, 4, 5]
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
     }
    }
  }

  